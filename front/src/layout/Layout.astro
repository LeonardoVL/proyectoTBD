---
interface Props {
    title: string;
}

const { title } = Astro.props;

---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
		<script src="https://kit.fontawesome.com/652c08c25b.js" crossorigin="anonymous"></script>
	</head>
	<body class="bg-[#F4F1F8]" >
		<slot />
	</body>
	
</html>
